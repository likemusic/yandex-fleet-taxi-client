{
  "name": "likemusic/yandex-fleet-taxi-client",
  "description": "Simple client for https://fleet.taxi.yandex.ru/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "proprietary",
  "authors": [
    {
      "name": "Valerij Ivashchenko",
      "email": "likemusicdev@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Likemusic\\YandexFleetTaxiClient\\": "src/",
      "Likemusic\\YandexFleetTaxiClient\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.3",
    "ext-libxml": "*",
