{
  "name": "yandex-fleet-taxi/client",
  "description": "Simple client for https://fleet.taxi.yandex.ru/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "proprietary",
  "authors": [
    {
      "name": "Valerij Ivashchenko",
      "email": "likemusicdev@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "YandexFleetTaxi\\Client\\": "src/",
      "YandexFleetTaxi\\Client\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.3",
    "ext-libxml": "*",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "yandex-fleet-taxi/client-contracts": "*",
    "php-http/client-common": "^2.0",
    "php-http/discovery": "^1.7"
  },
  "require-dev": {
    "php-http/curl-client": "^2.0",
    "guzzlehttp/psr7": "^1.6",
    "http-interop/http-factory-guzzle": "^1.0",
    "phpunit/phpunit": "^8"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:yandex-fleet-taxi/client-contracts.git"
    }
  ]
}
